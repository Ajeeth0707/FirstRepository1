package Exceptions;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class Collections {
	public static void main(String args)
	{
		
		Scanner s =new Scanner(System.in);
		
		
		
		List<String> l2=new ArrayList<>(5);
		for (int i=0;i<5;i++)
		{
			l2.add(s.next(i));
		}
		
		
		
	}

}
